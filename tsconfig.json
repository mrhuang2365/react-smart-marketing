{
  "compilerOptions": {
    "outDir": "./outjs/",
    "esModuleInterop": false,
    "sourceMap": true,
    "module": "esnext",
    "target": "es5",
    "experimentalDecorators": true,
    "skipLibCheck": true,
    "downlevelIteration": true,
    "moduleResolution": "node",
    "jsx": "preserve",
    "lib": [
      "es2015",
      "dom"
    ],
    "allowJs": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "strictNullChecks": true,
    "forceConsistentCasingInFileNames": true,
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "typeRoots": [
      "./src/types",
      "./node_modules/@types"
    ],
    "baseUrl": "./"
  },
  "include": [
    "./src/**/*.ts",
    "./src/**/*.tsx"
  ],
  "exclude": [
    "node_modules",
    "dist",
    ".imflow"
  ]
}
